set number
set title
set ambiwidth=double

set tabstop=4
set expandtab
set shiftwidth=4
set smartindent

set hidden

syntax on

set splitbelow
set splitright
set noequalalways

let g:netrw_alto = 1
let g:netrw_altv = 1
let g:netrw_liststyle=3
let g:netrw_chgwin = 2
let g:netrw_winsize = -25

set encoding=utf-8
set fileencodings=utf-8,sjis,euc-jp,iso-2022-jp
set fileformats=unix,dos,mac

set list

